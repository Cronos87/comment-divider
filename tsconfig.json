{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6"],
    "sourceMap": true,
    "rootDir": "src",
    "strict": false,
    "removeComments": true,
    "noUnusedLocals": false,
    "plugins": [{ "name": "typescript-tslint-plugin" }]
  },

  "exclude": ["node_modules", ".vscode-test"]
}
